<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*.meta;*.pyc;*.pyo;*.rbc;*.yarb;*~;.DS_Store;.git;.hg;.svn;CVS;__pycache__;_svn;vssver.scc;vssver2.scc;" />
    <extensionMap>
      <mapping ext="bytes" type="PLAIN_TEXT" />
      <mapping ext="all" type="PLAIN_TEXT" />
      <mapping pattern="*.lua.txt" type="lua" />
    </extensionMap>
  </component>
</application>